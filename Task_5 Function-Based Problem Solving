   "id": "574d2b4c-08b6-455f-a7bd-bdc445cf9fe2",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Total Bill: 1150\n",
      "Usage Level: Moderate Usage\n"
     ]
    }
   ],
   "source": [
    "#10.Smart Electricity Bill Analyzer\n",
    "\n",
    "def calculate_electricity_bill(units):\n",
    "    bill = 0\n",
    "    \n",
    "    if units <= 100:\n",
    "        bill = units * 3\n",
    "    elif units <= 200:\n",
    "        bill = (100 * 3) + (units - 100) * 5\n",
    "    else:\n",
    "        bill = (100 * 3) + (100 * 5) + (units - 200) * 7\n",
    "    \n",
    "    # Usage classification\n",
    "    if bill < 500:\n",
    "        usage = \"Low Usage\"\n",
    "    elif bill <= 1500:\n",
    "        usage = \"Moderate Usage\"\n",
    "    else:\n",
    "        usage = \"High Usage\"\n",
    "    \n",
    "    print(\"Total Bill:\", bill)\n",
    "    print(\"Usage Level:\", usage)\n",
    "\n",
    "\n",
    "calculate_electricity_bill(250)"
   ]
  }
