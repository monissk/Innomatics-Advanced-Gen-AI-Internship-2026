   ],
   "source": [
    "#4.Duplicate Account Detection\n",
    "\n",
    "def check_duplicates(usernames):\n",
    "    if len(usernames) != len(set(usernames)):\n",
    "        result = \"Yes\"\n",
    "    else:\n",
    "        result = \"No\"\n",
    "    \n",
    "    print(\"Duplicate Accounts Found:\", result)\n",
    "\n",
    "\n",
    "users = [\"user1\", \"user2\", \"user1\", \"user3\"]\n",
    "check_duplicates(users)"
   ]
